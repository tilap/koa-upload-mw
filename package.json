    "babel-loader": "^6.2.9",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-import": "^2.2.0"
  },
  "dependencies": {
